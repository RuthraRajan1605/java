package package1;

public class EvenOddArray {

	public static void main(String[] args) {
		int a[]= {2,4,6,1,3,5};
		for(int i=0;i<a.length;i++) {
			if(a[i]%2==0) {
				System.out.println(a[i]);
			}
		}
		for(int i=0;i<a.length;i++) {
			if(a[i]%2!=0) {
				System.out.println(a[i]);
			}
		}
	}

}
