package package1;

public class CountEvenOdd {

	public static void main(String[] args) {
		int num=15902;
		int even=0;
		int odd=0;
		while(num>0)
		{
			int rem=num%10;
			if(rem%2==0) {
				even++;
				
			}
			else {
				odd++;
			}
			num/=10;
			
		}
		System.out.println(even);
		System.out.println(odd);

	}

}
